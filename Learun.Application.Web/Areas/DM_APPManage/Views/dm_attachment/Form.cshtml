@{
    ViewBag.Title = "附件管理";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<link href="~/Content/webuploader/webuploader.css" rel="stylesheet" />
@Html.AppendCssFile("/Areas/DM_APPManage/Views/dm_attachment/Form.css")
<div class="lr-form-wrap" id="form">
    <div class="col-xs-12 lr-form-item">
        <div class="lr-form-item-title">附件简称<font face="宋体">*</font></div>
        <input id="file_custom_name" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="lr-wrap col-xs-12 lr-form-item">
        <div class="lr-form-item-title">图片<font face="宋体">*</font></div>
        <div class="file">
            <input type="file" name="uploadFile" id="uploadFile">
        </div>
    </div>
</div>
<script src="~/Content/webuploader/webuploader.nolog.min.js"></script>
@Html.AppendJsFile("/Views/LR_Content/script/lr-fileupload.js", "/Areas/DM_APPManage/Views/dm_attachment/Form.js")
