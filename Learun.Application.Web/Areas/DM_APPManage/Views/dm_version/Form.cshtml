@{
    ViewBag.Title = "版本管理";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<link href="~/Content/webuploader/webuploader.css" rel="stylesheet" />
@Html.AppendCssFile("/Areas/DM_APPManage/Views/DM_Banner/Form.css")
<div class="lr-form-wrap" id="form">
    <div class="col-xs-12 lr-form-item">
        <div class="lr-form-item-title">所属平台<font face="宋体">*</font></div>
        <div id="App_Plaform"><ul><li data-value="1">安卓</li><li data-value="2">IOS</li></ul></div>
    </div>
    <div class="col-xs-12 lr-form-item">
        <div class="lr-form-item-title">版本号<font face="宋体">*</font></div>
        <input id="App_Version" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-12 lr-form-item">
        <div class="lr-form-item-title">更新日志<font face="宋体">*</font></div>
        <textarea id="App_UpdateRemark" type="text" rows="5" class="form-control" isvalid="yes" checkexpession="NotNull"></textarea>
    </div>
    <div class="col-xs-12 lr-form-item">
        <div class="lr-form-item-title">应用包<font face="宋体">*</font></div>
        <div class="file">
            <div style="border: 1px solid #f3eeee;padding: 10px;width:110px;text-align:center;"><img src="" id="uploadPreview" /><div id="app_name"></div></div>
            <input type="file" name="uploadFile" id="uploadFile">
        </div>
    </div>
</div>
<script src="~/Content/webuploader/webuploader.nolog.min.js"></script>
@Html.AppendJsFile("/Views/LR_Content/script/lr-fileupload.js", "/Areas/DM_APPManage/Views/dm_version/Form.js")
